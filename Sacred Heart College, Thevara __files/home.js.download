function news() {
    document.getElementById("cl2").style.display = "none";
    document.getElementById("cl3").style.display = "none";
    var e = document.getElementById("cl1");
    if (e.style.display == 'block')
        e.style.display = 'none';
    else
        e.style.display = 'block';
    // var i = document.getElementById("find-ar");
    var re = document.getElementById("ar-right");
    var le = document.getElementById("ar-left");
    var hid = document.querySelector(".new-s1");
    if (le.style.display == 'none') {
        le.style.display = 'block';
        re.style.display = 'none';
        hid.style.display = 'none';
    } else {
        re.style.display = 'block';
        le.style.display = 'none';
        hid.style.display = 'block';
    }
    var re2 = document.getElementById("ar-right2");
    var le2 = document.getElementById("ar-left2");
    re2.style.display = 'block';
    le2.style.display = 'none';
    var re3 = document.getElementById("ar-right3");
    var le3 = document.getElementById("ar-left3");

    re3.style.display = 'block';
    le3.style.display = 'none';
    var re4 = document.getElementById("ar-right4");
    var le4 = document.getElementById("ar-left4");

    
    var hid2 = document.querySelector(".new-s2");
    hid2.style.display = 'block';

    var hid3 = document.querySelector(".new-s3");
    hid3.style.display = 'block';
    var hid4 = document.querySelector(".new-s4");
    hid4.style.display = 'block';

}


